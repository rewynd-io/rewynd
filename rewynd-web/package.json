    "react-cron-generator": "^2.0.19",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-grid-layout": "^1.4.4",
    "react-overlays": "^5.2.1",
    "react-player": "^2.14.1",
    "react-redux": "^9.1.0",
    "react-router": "^7.0.0",
    "react-router-dom": "^7.0.0",
